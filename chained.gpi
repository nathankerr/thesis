set style data linespoints
set logscale
set xtics (1,8,16,32,64,128,256,512,1024)
set ytics (1,8,16,32,64,128,256,512,1024)
set xlabel 'Number of Processors (Log Scale)'
set ylabel 'Speedup (Log Scale)'
set xrange [1:1024]
set yrange [1:1024]
l(x) = x
plot l(x) title 'Ideal Speedup', 'hadoopGIS-chained.gplot' using 1:3 title 'hadoopGIS (60%)', 'clusterGIS-chained.gplot' using 1:3 title 'clusterGIS'
